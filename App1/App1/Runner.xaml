        <Image x:Name="imgButtonClick" Source="images/ButtonClick4.png" HorizontalAlignment="Left" Height="76" VerticalAlignment="Top" Width="55" Margin="71,481,0,0"/>
        <Button x:Name="btnBack" Content="Back" HorizontalAlignment="Left" Margin="76,487,0,0" VerticalAlignment="Top" Click="btnBack_Click"/>
        <Button x:Name="btnHighScore" Content="Highscores" HorizontalAlignment="Left" Margin="198,487,0,0" VerticalAlignment="Top"/>
        <Image x:Name="imgBackgroundMain" Source="images/StickmanBackground.png" HorizontalAlignment="Left" Height="295" VerticalAlignment="Top" Width="371" Margin="0,0,-11,0"/>
        <Image x:Name="imgStickman" Source="images/StickmanRunningStage1.png" HorizontalAlignment="Left" Height="158" VerticalAlignment="Top" Width="198" Margin="0,295,0,0"/>

    </Grid>
